        <activity android:name=".login.FindpwActivity">
        </activity>
        <activity android:name=".login.FindidActivity">
        </activity>
        <activity android:name=".login.SignupActivity">
        </activity>
        <activity android:name=".main.MainActivity">
        </activity>
